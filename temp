touch gpsdservice
uci set gpsdservice.venus=gpsdservice
uci set gpsdservice.venus.ip=192.168.10.20:8500
uci commit gpsdservice
uci show gpsdservice

wget -O /etc/init.d/gpsdservice https://raw.githubusercontent.com/thespinmaster/GLX3000-GPS/refs/heads/main/gpsdservice
wget -O /etc/config/gpsdservice https://raw.githubusercontent.com/thespinmaster/GLX3000-GPS/refs/heads/main/gpsdservice.config





wget -O /etc/config/gpsdservice https://raw.githubusercontent.com/thespinmaster/GLX3000-GPS/refs/heads/main/gpsdservice-install

chmod 775 gpsdservice-install.sh
